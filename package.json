{
  "name": "monti-apm-core",
  "version": "1.7.5",
  "description": "Monti APM Core API",
  "main": "./dist/",
  "scripts": {
    "clean": "rimraf dist",
    "build": "swc $(glob 'src/**/*.{js,ts}' --nodir --ignore 'src/**/*.test.{js,ts}' --ignore 'src/tests/**/*') --out-dir=dist",
    "types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "lint": "eslint src --cache",
    "lintfix": "eslint src --fix",
    "testonly": "mocha src/**/*.test.js --watch=false --exit",
    "test": "npm run lint && npm run testonly",
    "test:watch": "nodemon --exec 'npm run testonly' -e js --watch src",
    "prepublish": "yarn clean && yarn build && yarn types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monti-apm/monti-apm-core.git"
  },
  "keywords": [
    "kadira",
    "monti apm"
  ],
  "author": "Monti APM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monti-apm/monti-apm-core/issues"
  },
  "homepage": "https://github.com/monti-apm/monti-apm-core#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.71",
    "@swc/register": "^0.1.10",
    "@types/backoff": "^2.5.2",
    "@types/chai": "^4.3.4",
    "@types/debug": "^4.1.8",
    "@types/mocha": "^10.0.1",
    "@types/uuid": "^9.0.2",
    "@types/websocket": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "^4.3.7",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "^4.17.1",
    "glob": "^10.3.3",
    "mocha": "10.2.0",
    "nodemon": "^2.0.21",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.5",
    "ws": "^8.13.0"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "backoff": "^2.5.0",
    "debug": "^3.2.6",
    "eventemitter2": "^6.4.9",
    "faye-websocket": "^0.11.4",
    "uuid": "^9.0.0"
  },
  "volta": {
    "node": "14.21.2",
    "yarn": "1.22.17"
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
