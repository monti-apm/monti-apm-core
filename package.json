{
  "name": "monti-apm-core",
  "version": "1.7.5",
  "description": "Monti APM Core API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "swc src --out-dir=dist",
    "types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "lint": "eslint src --cache",
    "lintfix": "eslint src --fix",
    "test:dist": "mocha 'dist/**/*.test.js' --exit --watch=false",
    "test": "mocha src/**/*.test.js --require=@swc/register --exit --watch=false",
    "test:watch": "nodemon --exec 'npm run test' -e js --watch src",
    "test:debug": "DEBUG=kadira-core:* npm run test:watch",
    "prepublishOnly": "npm run clean && npm run build && npm run types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monti-apm/monti-apm-core.git"
  },
  "keywords": [
    "kadira",
    "monti apm"
  ],
  "author": "Monti APM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monti-apm/monti-apm-core/issues"
  },
  "homepage": "https://github.com/monti-apm/monti-apm-core#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.71",
    "@swc/register": "^0.1.10",
    "@types/chai": "^4.3.4",
    "@types/debug": "^4.1.8",
    "@types/mocha": "^10.0.1",
    "@types/uuid": "^9.0.2",
    "@types/websocket": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "4.3.7",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "4.18.2",
    "glob": "^7.1.3",
    "mocha": "10.2.0",
    "nodemon": "2.0.21",
    "prettier": "2.8.8",
    "rimraf": "^4.4.1",
    "typescript": "4.9.5",
    "ws": "8.13.0"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "debug": "^3.2.6",
    "eventemitter2": "^6.4.9",
    "faye-websocket": "^0.11.4",
    "remeda": "^1.25.1"
  },
  "volta": {
    "node": "20.5.0"
  }
}
