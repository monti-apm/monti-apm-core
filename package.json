{
  "name": "@monti-apm/core",
  "version": "2.0.0-beta.7",
  "description": "Monti APM Core API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint src --cache",
    "lintfix": "eslint src --fix",
    "test:dist": "mocha 'dist/**/*.test.js' --exit",
    "test": "mocha src/**/*.test.js --require=ts-node/register/transpile-only --exit",
    "test:watch": "nodemon --exec 'npm run test' -e js,ts --watch src",
    "test:debug": "cross-env DEBUG=monti-apm-core:* npm run test:watch",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monti-apm/monti-apm-core.git"
  },
  "keywords": [
    "kadira",
    "monti apm"
  ],
  "author": "Monti APM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monti-apm/monti-apm-core/issues"
  },
  "homepage": "https://github.com/monti-apm/monti-apm-core#readme",
  "devDependencies": {
    "@types/debug": "^4.1.10",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "4.18.2",
    "glob": "^7.1.3",
    "mocha": "6.2.3",
    "nodemon": "2.0.21",
    "prettier": "2.8.8",
    "rimraf": "^4.4.1",
    "sinon": "^15.2.0",
    "ts-node": "^9.1.1",
    "typescript": "4.9.5",
    "ws": "6.2.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "debug": "^3.2.6",
    "eventemitter2": "^6.4.9",
    "faye-websocket": "^0.11.4",
    "https-proxy-agent": "^7.0.5"
  },
  "volta": {
    "node": "20.9.0"
  }
}
